<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Resource Dictionary for colors 
    
    Do not modify above this line, period.-->

    <!-- Foreground Color is used as the primary color for text. -->
    <Color x:Key="ForegroundColor">#ffffff</Color>

    <!-- Highlight color is used to draw attention to controls, such as when a control is focused. -->
    <Color x:Key="HighlightColor">#0066cc</Color>
    
    <!-- Contrast color is used in some places where a contrasting color is needed, such as for the friend's nameplates -->
    <Color x:Key="ContrastColor">#ffb400</Color>

    <!-- Background is used as the basis for every UI element. -->
    <Color x:Key="BackgroundColor">#000022</Color>

    <!-- Depressed color is used primarily used for borders and UI controls which cannot be interacted with. -->
    <Color x:Key="DepressedColor">#004488</Color>

    <!-- Bright background is used for some odds and ends, most notably the 'expand' bars on either side of the screen. -->
    <Color x:Key="BrightBackgroundColor">#00133b</Color>
	
	<Color x:Key="MaleColor">#6699FF</Color>
	<Color x:Key="FemaleColor">#FF6699</Color>
	<Color x:Key="CuntColor">#00CC66</Color>
	<Color x:Key="MaleHermColor">#007FFF</Color>
	<Color x:Key="TransgenderColor">#EE8822</Color>
	<Color x:Key="ShemaleColor">#CC66FF</Color>
	<Color x:Key="HermColor">#9B30FF</Color>


    <!-- Do not modify below this line unless you know what you are doing. -->

    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}" />
    <SolidColorBrush x:Key="HighlightBrush" Color="{StaticResource HighlightColor}" />
    <SolidColorBrush x:Key="ContrastBrush" Color="{StaticResource ContrastColor}"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="DepressedBrush" Color="{StaticResource DepressedColor}" />
    <SolidColorBrush x:Key="BrightBackgroundBrush" Color="{StaticResource BrightBackgroundColor}" />
	
	<SolidColorBrush x:Key="MaleBrush" Color="{StaticResource MaleColor}" />
	<SolidColorBrush x:Key="FemaleBrush" Color="{StaticResource FemaleColor}" />
	<SolidColorBrush x:Key="HermBrush" Color="{StaticResource HermColor}" />
	<SolidColorBrush x:Key="MaleHermBrush" Color="{StaticResource MaleHermColor}" />
	<SolidColorBrush x:Key="TransgenderBrush" Color="{StaticResource TransgenderColor}" />
	<SolidColorBrush x:Key="CuntBrush" Color="{StaticResource CuntColor}" />
	<SolidColorBrush x:Key="ShemaleBrush" Color="{StaticResource ShemaleColor}" />
</ResourceDictionary>